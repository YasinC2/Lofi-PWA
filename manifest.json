{
  "name": "LOFI",
  "short_name": "LOFI",
  "description": "Generate and play unique lofi tracks using machine learning and interactive sliders.",
  "start_url": "/Lofi-PWA/",
  "scope": "/",
  "protocol_handlers": [
    {
      "protocol": "web+lofi",
      "url": "/Lofi-PWA/?url=%s"
    }
  ],
  "display": "standalone",
  "background_color": "#181818",
  "theme_color": "#fc5c8c",
  "orientation": "any",
  "icons": [
    {
      "src": "manifest-icon-196.maskable.png",
      "sizes": "192x192",
      "type": "image/png",
      "purpose": "any maskable"
    },
    {
      "src": "manifest-icon-512.maskable.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "any maskable"
    }
  ],
  "screenshots": [
    {
      "src": "screenshot-desktop.jpg",
      "sizes": "1280x800",
      "type": "image/jpeg",
      "form_factor": "wide"
    },
    {
      "src": "screenshot-mobile.jpg",
      "sizes": "540x720",
      "type": "image/jpeg",
      "form_factor": "narrow"
    }
  ],
  "lang": "en",
  "dir": "auto",
  "categories": [
    "music",
    "entertainment",
    "lifestyle"
  ]
}
